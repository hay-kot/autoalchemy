[flake8]
max-line-length = 120
ignore = [
    E501 # Line Length - See Black Config in pyproject.toml
    E402 # Import Not at Top of File
]
per-file-ignores = 
    __init__.py:F403,F401

exclude =
    .git,
    __pycache__,
    setup.py,
    build,
    dist,
    releases,
    .venv,
    .tox,
    .mypy_cache,
    .pytest_cache,
    .vscode,
    .github,
    tests

exclude = _all_models.py

